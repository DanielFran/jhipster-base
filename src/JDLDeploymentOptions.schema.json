{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "array",
    "title": "The Root Schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": [],
    "additionalItems": true,
    "items": {
        "$id": "#/items",
        "type": "object",
        "title": "The Items Schema",
        "description": "An explanation about the purpose of this instance.",
        "default": {},
        "examples": [
            {
                "values": [
                    "\"docker push\""
                ],
                "description": "The docker push command to use. Must be in double quotes",
                "parseType": "AnyLiteral",
                "option": "dockerPushCommand",
                "baseType": "STRING"
            },
            {
                "values": [
                    "default"
                ],
                "description": "Applicable only when deploymentType is kubernetes",
                "parseType": "AnyLiteral",
                "option": "kubernetesNamespace",
                "baseType": "STRING"
            },
            {
                "option": "serviceDiscoveryType",
                "baseType": "STRING",
                "values": [
                    "eureka",
                    "consul",
                    "no"
                ],
                "description": "",
                "parseType": "Literal"
            }
        ],
        "additionalProperties": true,
        "required": [
            "option",
            "baseType",
            "parseType",
            "values",
            "description"
        ],
        "properties": {
            "option": {
                "$id": "#/items/properties/option",
                "type": "string",
                "title": "The Option Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "dockerPushCommand"
                ]
            },
            "baseType": {
                "$id": "#/items/properties/baseType",
                "type": "string",
                "title": "The Basetype Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "STRING"
                ]
            },
            "parseType": {
                "$id": "#/items/properties/parseType",
                "type": "string",
                "title": "The Parsetype Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "AnyLiteral"
                ]
            },
            "values": {
                "$id": "#/items/properties/values",
                "type": "array",
                "title": "The Values Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": [],
                "examples": [
                    [
                        "\"docker push\""
                    ]
                ],
                "additionalItems": true,
                "items": {
                    "$id": "#/items/properties/values/items",
                    "type": "string",
                    "title": "The Items Schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "\"docker push\""
                    ]
                }
            },
            "description": {
                "$id": "#/items/properties/description",
                "type": "string",
                "title": "The Description Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "The docker push command to use. Must be in double quotes"
                ]
            }
        }
    }
}